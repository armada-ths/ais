{
    "name": "complete_registration",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "npx concurrently \"vite --host\" \"npx tailwindcss -i ./src/input.css -o ./output.css --watch\"",
        "build": "mkdir -p ../../ais_static/apps/complete_registration && rm -rf ../../ais_static/apps/complete_registration/* && tsc && npx tailwindcss -i ./src/input.css -o ./output.css && vite build && cat ./output.css >> ../../ais_static/apps/complete_registration/assets/index.css",
        "lint": "eslint --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "prettier-check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,css,md}'",
        "preview": "vite preview"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.5",
        "@tanstack/react-router": "0.0.1-beta.151",
        "clsx": "^2.0.0",
        "luxon": "^3.4.0",
        "prettier": "^3.0.1",
        "prettier-plugin-tailwindcss": "^0.4.1",
        "primeicons": "^6.0.1",
        "primereact": "^9.6.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.1.2",
        "reselect": "^4.1.8",
        "tailwind-merge": "^1.14.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/luxon": "^3.3.1",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.0.3",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.45.0",
        "eslint-plugin-prefer-function-declarations": "^2.1.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "postcss": "^8.4.27",
        "tailwindcss": "^3.3.3",
        "typescript": "^5.0.2",
        "vite": "^4.4.5"
    }
}
