{
    "name": "dashboard",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "npx concurrently \"vite --host\" \"npx tailwindcss -i ./src/input.css -o ./output.css --watch\"",
        "build-prod": "tsc && npx tailwindcss -i ./src/input.css -o ./output.css && vite build && cat ./output.css >> dist/assets/index.css",
        "build": "pnpm build-prod && mkdir -p ../../ais_static/apps/dashboard && rm -rf ../../ais_static/apps/dashboard/* && cp -r ./dist/* ../../ais_static/apps/dashboard",
        "ci-eslint-check": "eslint --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "ci-prettier-check": "prettier --check",
        "preview": "vite preview"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.3.4",
        "@radix-ui/react-alert-dialog": "^1.0.5",
        "@radix-ui/react-checkbox": "^1.0.4",
        "@radix-ui/react-collapsible": "^1.0.3",
        "@radix-ui/react-dialog": "^1.0.5",
        "@radix-ui/react-label": "^2.0.2",
        "@radix-ui/react-popover": "^1.0.7",
        "@radix-ui/react-select": "^2.0.0",
        "@radix-ui/react-separator": "^1.0.3",
        "@radix-ui/react-slot": "^1.0.2",
        "@radix-ui/react-tabs": "^1.0.4",
        "@reduxjs/toolkit": "^1.9.5",
        "@tanstack/react-query": "^5.22.2",
        "@tanstack/react-router": "1.19.1",
        "@tanstack/router-devtools": "^1.19.1",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "cmdk": "^1.0.0",
        "jotai": "^2.7.0",
        "lucide-react": "^0.335.0",
        "luxon": "^3.4.0",
        "next-themes": "^0.2.1",
        "prettier": "^3.0.1",
        "prettier-plugin-tailwindcss": "^0.4.1",
        "primeicons": "^6.0.1",
        "primereact": "^9.6.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.50.1",
        "react-redux": "^8.1.2",
        "react-responsive-combo-box": "^1.3.0",
        "reselect": "^4.1.8",
        "sonner": "^1.0.3",
        "tailwind-merge": "^1.14.0",
        "tailwindcss-animate": "^1.0.7",
        "vaul": "^0.9.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/luxon": "^3.3.1",
        "@types/node": "^20.11.19",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.0.3",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.45.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "postcss": "^8.4.27",
        "tailwindcss": "^3.3.3",
        "typescript": "5.4.0-beta",
        "vite": "^4.4.5"
    }
}
